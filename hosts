## Ansible Hosts File
[control]
usr1control ansible_host=10.0.0.101 ansible_ssh_pass='ChangeMe1234' ansible_ssh_user='root'

[puppets]
puppet1 ansible_host=10.0.0.103 ansible_ssh_pass='ChangeMe1234' ansible_ssh_user='root'
puppet2 ansible_host=10.0.0.102 ansible_ssh_pass='ChangeMe1234' ansible_ssh_user='root'
